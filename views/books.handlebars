<h1>Títulos disponíveis</h1>
<div class="books-container">
    <a href="/">Adicionar Livro</a>
  {{#each books}}
    <div>
      {{this.id}}
      -
      <span class="bold"></span>
      <a href="/books/{{this.id}}">{{this.title}}</a>-
      {{this.pages}}
      páginas
    </div>
  {{/each}}
</div>